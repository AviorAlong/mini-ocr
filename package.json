  "description": "ocr  ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "寅恪OCR",
    "appId": "com.electron.along",
    "copyright": "avior along 所属",
    "directories": {
      "output": "build"
    },
    "nsis": {
